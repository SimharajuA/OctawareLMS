@model Employee
@{
    ViewData["Title"] = "Details";
}

<h1>Manager Details</h1>
<div class="container-fluid">
    <div class="align-middle">
        <table>
            <tr>
                <th>Manager Id</th>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <th>Manager Name</th>
                <td>@Model.Name</td>
            </tr>
            <tr>
                <th>Manager Date Of Joining</th>
                <td>@Model.DateOfJoining.Date</td>
            </tr>
            <tr>
                <th>Manager Date Of Birth</th>
                <td>@Model.DateOfBirth.Date</td>
            </tr>
        </table>
    </div>
</div>

